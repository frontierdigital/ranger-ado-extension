{
  "name": "ranger-ado-extension",
  "version": "0.0.28",
  "description": "",
  "scripts": {
    "build": "node ./scripts/build.js",
    "ci": "run-p ci:*",
    "ci:tasks": "find ./tasks -type d -regex \".*V[0-9]\" -maxdepth 2 -exec npm --prefix {} ci \\;",
    "install": "run-p install:*",
    "install:tasks": "find ./tasks -type d -regex \".*V[0-9]\" -maxdepth 2 -exec npm --prefix {} install \\;",
    "test": "run-p test:*",
    "test:tasks": "find ./tasks -type d -regex \".*V[0-9]\" -maxdepth 2 -exec npm --prefix {} test \\;"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:frontierdigital/ranger-ado-extension.git"
  },
  "author": "Frontier Digital Ltd",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/frontierdigital/ranger-ado-extension/issues"
  },
  "homepage": "https://github.com/frontierdigital/ranger-ado-extension#readme",
  "devDependencies": {
    "eslint": "^8.44.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.27.5",
    "fs-extra": "^11.1.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^5.0.1",
    "shelljs": "^0.8.5",
    "tempdir": "^2.0.0",
    "tfx-cli": "^0.15.0",
    "winston": "^3.10.0"
  }
}
